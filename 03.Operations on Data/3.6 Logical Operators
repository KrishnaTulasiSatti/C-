#include<iostream>
using namespace std;
int main()
{  int d = 45,e = 20,f=11,h=0;
   cout <<  (d&&e) << endl;     //  **d&&e--> 1 because other than 0 remaining any numbers are true
//We have to use braces otherwise we get error
   cout <<  (d&&h) << endl;       // 0               
   cout <<  ((d>e)&&(d>f))  << endl; //1
   cout <<  (!(d>e)&&(d>f))  << endl; //0
   cout <<  ((d>e)||(d>f))  << endl;  //1
   bool a=true,b=false,c=true;
   cout <<  (!(a&&b)||c)  << endl;  //1
}
