#include<iostream>
using namespace std;
int main(){
	 int a=3;
	 cout<<a<<endl;//3
	 a=5;
	 cout<<a<<endl;//5
	 double b=99.99;
	 cout<<b<<endl;//99.99
	 b=67.89;
	 cout<<b<<endl;//67.89
	 bool flag=false;
	 cout<<boolalpha<<flag<<endl; //false
	 flag=true;
	 cout<<boolalpha<<flag<<endl; //true
	 //Careful about auto assignments
	 auto c=333u;
	 c=-22;
	 cout<<c; //garbage value
      
	}
