#include<iostream>
using namespace std;
int main(){
	  auto var1=12;
	  auto var2=13.0;
      auto var3=14.0f;
      auto var4=15.0l;
      auto var5='e';
      auto var6=123u;   //unsigned
      auto var7=123ul;  //unsigned long
      auto var8=123ll;  // long long
      cout<<sizeof(var1)<<endl; //4
      cout<<sizeof(var2)<<endl; //8
      cout<<sizeof(var3)<<endl; //4
      cout<<sizeof(var4)<<endl; //16
      cout<<sizeof(var5)<<endl; //1
      cout<<sizeof(var6)<<endl; //4
      cout<<sizeof(var7)<<endl; //8
      cout<<sizeof(var8)<<endl; //8
      
	}
